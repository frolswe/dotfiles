Host *.local
	CheckHostIP no

Host hupp.se
        DynamicForward 3128
	LocalForward 9050 localhost:9050
Host hupp.se *.hupp.se
	User frol
	ForwardAgent yes
	ForwardX11 yes

Host *
	ControlPath ~/.ssh/master-%l-%r@%h:%p
	ControlMaster auto
	ForwardAgent no
	ForwardX11 no
	NoHostAuthenticationForLocalhost no
	ServerAliveInterval 20
	VerifyHostKeyDNS yes
