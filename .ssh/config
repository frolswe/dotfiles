Host *.local
	CheckHostIP no

Host *.onion
	VerifyHostKeyDNS no
	ProxyCommand nc -x localhost:9050 -X 5 %h %p

Host *
	User frol
	ControlPath ~/.ssh/master-%l-%r@%h:%p
	ControlMaster auto
	ControlPersist 10m
	ForwardAgent no
	ForwardX11 no
	NoHostAuthenticationForLocalhost yes
	ServerAliveInterval 20
	VerifyHostKeyDNS yes
